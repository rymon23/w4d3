
<form action="<%= users_url %>" method="POST">

<h2>Sign-up</h2>

<label for="user_name">Username:</label>
<input 
  type="text"
  name="user[user_name]"
  id="user_name">
<br>
<br>
<label for="password">Password:</label>
<input 
  type="text"
  name="user[password]"
  id="password">
<br>
<br>
<input type="submit" value="Sign-up">
</form>